Common.MouseButton1Click:connect(function()
	DeleteCommon = not DeleteCommon
	if DeleteCommon then 
		Common.BackgroundColor3 = Color3.new(0,1,0)
	else
		Common.BackgroundColor3 = Color3.new(1,0,0)
	end
end)

