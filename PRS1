	local hb = game:GetService("RunService").Heartbeat
while true do
	wait(0.1)
	if DeleteACommon then
		ACommonDeletus()
	end
	if DeleteUncommon then
		AUncommonDeletus()
	end
	if DeleteARare then
		ARareDeletus()
	end
	if DeleteAEpic then
		AEpicDeletus()
	end
	if DeleteALegendary then
		ALegendaryDeletus()
	end
	if DeleteAPremium then
		APremiumDeletus()
	end
end)

