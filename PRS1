	while true do
		if Open1 then
			Open1Function()
		end
		if Open2 then
			Open2Function()
		end
		if Open3 then
			Open3Function()
		end
		if Open4 then
			Open4Function()
		end
		if Open5 then
			Open5Function()
		end
		if OpenRebirth then 
			OpenRebirthFunction()
		end
		hb:wait()
	end
end))

